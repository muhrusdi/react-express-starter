{
  "name": "react-express-starter",
  "version": "1.0.0",
  "main": "index.js",
  "author": "muhrusdi",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --sequential dev:build-server --parallel dev:*",
    "build": "npm-run-all --parallel build:*",
    "dev:start": "nodemon --watch build --exec \"node public/server.js\"",
    "dev:build-server": "cross-env webpack --mode development --config webpack.config.server.js",
    "dev:watch-server": "cross-env webpack --mode development --config webpack.config.server.js --watch",
    "dev:watch-client": "cross-env webpack-dev-server --mode development --config webpack.config.client.js --open --hot --colors --inline",
    "build:start": "nodemon --watch build --exec \"node public/server.js\"",
    "build:server": "cross-env webpack --mode production --config webpack.config.server.js --progress --hide-modules",
    "build:client": "cross-env webpack --mode production --config webpack.config.client.js --progress --hide-modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "express": "^4.16.2",
    "html-webpack-plugin": "^3.0.4",
    "image-webpack-loader": "^4.1.0",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  }
}
